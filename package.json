{
  "name": "mysql-mcp-webui",
  "version": "0.0.7",
  "description": "MySQL MCP server with React-based web UI for live configuration management",
  "workspaces": [
    "server",
    "client"
  ],
  "type": "module",
  "bin": {
    "mysql-mcp-webui": "server/dist/index.js"
  },
  "files": [
    "server/dist",
    "server/public",
    "data/.gitkeep",
    "README.md",
    "LICENSE",
    "DEPLOYMENT.md"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "npm run dev --workspace=server",
    "dev:client": "npm run dev --workspace=client",
    "build": "npm run build:client && npm run build:server",
    "build:server": "npm run build --workspace=server",
    "build:client": "npm run build --workspace=client",
    "start": "npm run start --workspace=server",
    "start:http": "TRANSPORT=http npm run start --workspace=server",
    "start:http:prod": "NODE_ENV=production TRANSPORT=http npm run start --workspace=server",
    "start:stdio": "TRANSPORT=stdio npm run start --workspace=server",
    "install:global": "npm run build && npm install -g .",
    "clean": "rm -rf node_modules server/node_modules client/node_modules server/dist client/dist",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "mcp",
    "mysql",
    "database",
    "claude",
    "model-context-protocol",
    "web-ui",
    "react"
  ],
  "author": "Yash Agarwal",
  "repository": {
    "type": "git",
    "url": "https://github.com/yashagldit/mysql-mcp-webui.git"
  },
  "bugs": {
    "url": "https://github.com/yashagldit/mysql-mcp-webui/issues"
  },
  "homepage": "https://github.com/yashagldit/mysql-mcp-webui#readme",
  "license": "MIT",
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/cookie-parser": "^1.4.10",
    "@types/jsonwebtoken": "^9.0.10",
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.21.0",
    "bcrypt": "^6.0.0",
    "better-sqlite3": "^12.4.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^5.0.1",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.11.5",
    "node-sql-parser": "^5.3.4",
    "zod": "^3.23.8"
  }
}
